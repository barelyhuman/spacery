{
  "name": "spacery",
  "version": "0.0.7",
  "description": "Space Modifier utilities for working with React",
  "keywords": [
    "react",
    "component",
    "components",
    "util",
    "utils",
    "utilities",
    "helper",
    "helpers",
    "spacer",
    "margins",
    "padding"
  ],
  "source": "src/index.js",
  "repository": "barelyhuman/spacery",
  "license": "MIT",
  "author": {
    "name": "Reaper",
    "email": "ahoy@barelyhuman.dev",
    "url": "https://barelyhuman.dev"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "wrap",
    "fix": "wrap --fix",
    "prepare": "npm run build && husky install",
    "prepublishOnly": "npm run build",
    "size": "sizesnap",
    "test": "ava"
  },
  "lint-staged": {
    "*.js": [
      "npx standard --fix"
    ]
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@barelyhuman/wrap": "^1.0.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/react": "^16.9.2",
    "ava": "^3.15.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "microbundle": "^0.14.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rollup": "^2.66.1",
    "rollup-plugin-node-externals": "^3.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "terser": "^5.10.0"
  },
  "peerDependencies": {
    "react": ">=16.7"
  },
  "engines": {
    "node": ">=12"
  },
  "wrap": {
    "inferExternal": true
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  }
}
